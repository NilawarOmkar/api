<div id="<%= dom_id user %>">
  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Tenant:</strong>
    <%= user.tenant_id %>
  </p>

  <p>
    <strong>Current tenant:</strong>
    <%= user.current_tenant_id %>
  </p>

</div>
